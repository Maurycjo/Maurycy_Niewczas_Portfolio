<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>

<BorderPane xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pwr.edu.pl.window.Controller">
   <bottom>
      <FlowPane alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="serializeButton" mnemonicParsing="false" onAction="#serialize" text="Serializuj" />
            <Button fx:id="deserializeButton" mnemonicParsing="false" onAction="#deserialize" text="Deserializuj" />
            <Button fx:id="loadDataButton" mnemonicParsing="false" onAction="#loadData" text="Wczytaj" />
         </children>
      </FlowPane>
   </bottom>
   <top>
      <TabPane fx:id="myTabPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="33.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab fx:id="jaxbTab" onSelectionChanged="#changedToJaxb" text="JAXB" />
          <Tab fx:id="jaxpTab" onSelectionChanged="#changedToJaxp" text="JAXP" />
            <Tab onSelectionChanged="#changedToXslt" text="XSLT" fx:id="xsltTab" />
        </tabs>
      </TabPane>
   </top>
   <center>
      <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="479.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <content>
            <TextArea fx:id="outputTextArea" prefHeight="200.0" prefWidth="200.0" />
         </content>
      </ScrollPane>
   </center>
</BorderPane>
